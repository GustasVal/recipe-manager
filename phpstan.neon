includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:

    paths:
        - app
        - config
        - database
        - resources
        - routes
        - tests
        - public/index.php

    # The level 8 is the highest level
    level: 7

    ignoreErrors:
        - '#Call to an undefined method Illuminate\\Support\\Collection::makeVisible\(\)#'

    excludes_analyse:
        - ./routes/console.php

    checkMissingIterableValueType: false
